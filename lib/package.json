{
  "name": "mocha-concurrent-api-tests",
  "version": "1.0.0",
  "description": "$todo-description",
  "main": "src/index.js",
  "directories": {
    "lib": "./src"
  },
  "scripts": {
    "start": "tsc && mocha \"src/**/*.test.js\" --require source-map-support/register --require mocha-error-reporters --reporter=minWithFullErrors --no-timeouts --exit",
    "watch": "tsc --watch --pretty",
    "lint": "node_modules/.bin/villemontreal-lint . check node both",
    "lint-fix": "node_modules/.bin/villemontreal-lint . fix node both",
    "prettier": "node_modules/.bin/villemontreal-lint . check node prettier",
    "prettier-fix": "node_modules/.bin/villemontreal-lint . fix node prettier",
    "tslint": "node_modules/.bin/villemontreal-lint . check node tslint",
    "tslint-fix": "node_modules/.bin/villemontreal-lint . fix node tslint"
  },
  "repository": {
    "type": "git",
    "url": "$todo-repository-url"
  },
  "homepage": "$todo-repository-url",
  "keywords": [
    "$todo-keywords"
  ],
  "author": "St√©phane Leblanc",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/lodash": "4.14.170",
    "@types/mocha": "8.2.0",
    "@villedemontreal/lint-config": "1.7.5",
    "mocha": "8.3.2",
    "mocha-error-reporters": "1.0.0",
    "source-map-support": "0.5.19",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "chai": "4.3.4",
    "lodash": "4.17.21",
    "mocha.parallel": "0.15.6",
    "uuid": "8.3.2"
  }
}
